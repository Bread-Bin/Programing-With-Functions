import math
import time

n_items = int(input("Enter the number of items: "))
n_per_box = int(input("Enter the number of items per box: "))
print()
time.sleep(1)
n_boxes = math.ceil(n_items / n_per_box)
print(f"For {n_items} items, packing {n_per_box}"
    f" items in each box, you will need {n_boxes} boxes.")

